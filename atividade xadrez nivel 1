#include <stdio.h>

int main(){

int torre = 1, bispo = 1, rainha =1, cavalo=1;

//torre
printf("torre\n");

while(torre <=5)
{
    printf("direita\n");
    torre++;
}


// rainha


do {
    printf("esquerda\n");
    rainha++;
}while (rainha <=8);


// bispo

for(int i = 1;  i<=5; i++)
{
    printf("cima e direita\n ");
}


// cavalo
printf("cavalo\n");

//loop

while(cavalo--)
{
    for(int i = 0;i <2; i++) {

    }
}


}
